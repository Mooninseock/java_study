

class C{
	int left,right;
	
	public void setOperhands(int left, int right) {
		this.left = left;
		this.right = right;
	}
	
	public void sum() {
		System.out.println(this.left+ this.right);
	}
	public void avg() {
		System.out.println((this.left+this.right)/2);
	}
}
class subtractableCalcultor extends C{
	public void subtract() {
	System.out.println(this.left-this.right);
}
}
public class Calculator {
	public static void main(String[] args) {
		
		C c2 = new C();
		c2.setOperhands(10 , 20);
		c2.sum();
		c2.avg();
		//c2.subtract();
		
 subtractableCalcultor c1 = new subtractableCalcultor();
 c1.setOperhands(100 ,50);
 c1.sum();
 c1.avg();
 c1.subtract();
	}
}

c2.subtract는 작동 하지 않기 때문에 주석 처리 함.
